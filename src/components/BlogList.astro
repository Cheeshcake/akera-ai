---
import PostCard from "../components/PostCard.astro"
const allPosts = await Astro.glob("../pages/posts/*.md");

import { getPostUrl } from "../js/utils.js";

---


<div>
  
  {
    allPosts.map((post) => (
      <PostCard
        frontmatter={post.frontmatter}
        url= {getPostUrl(post)}
      />
    ))
  }
</div>